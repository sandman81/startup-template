                width 0
                height 0
                border-style solid
                border-width 21.5px 0 21.5px 27px
                border-color transparent transparent transparent white
                @media mediumView
                    position relative
                    left -2px
                    border-width 10px 0 10px 12px
                @media smallView
                    border-width 8.5px 0 8.5px 10px
        .round-figure
            top 270px
            right 411px
            @media mediumView
                z-index 2
                top 115px
                right 110px
            @media smallView
                display none
        .square-figure
            right 0
            top 205px

