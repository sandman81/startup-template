.layout-header
    height 92px
    background white
    position fixed
    width 100%
    top 0
    left 0
    z-index 100
    @media mediumView
        .content-center
            margin 0
    @media smallView
        height 55px
        border-bottom 1px solid #cacaca
    
.header-content
    height 100%
    padding 0 30px
    @media mediumView
        padding-right 0
    &-buttons
        flex 1 0 0
        height 100%
        &_reg
            flex 0 0 130px
            .header-content-buttons > &
                @media smallView
                    display none
            .top-menu &
                border-radius 0
                height 50px
                display none
                @media smallView
                    display flex
        &_call
            cursor pointer
            margin-left 60px
            @media largeView
                display none
        &_burger
            flex 0 0 55px
            height 100%
            cursor pointer
            margin-left 40px
            span
                flex 0 0 18px
                height 1px
                background yellowColor
                position relative
                &:before,
                &:after
                    content ''
                    position absolute
                    left 0
                    width 100%
                    height 1px
                    background yellowColor
                &:before
                    top -6px
                &:after
                    bottom -6px
            @media largeView
                display none

.top-menu
    margin-right 45px
    @media mediumView
        position absolute
        margin 0
        width 100%
        display block
        left 0
        top 92px
        background #292929
        animation-duration 0.3s
        &:not(.active)
            display none
    @media smallView
        top 56px
    &_button
        cursor pointer
        user-select none
        font-size 10px
        text-transform uppercase
        margin-left 50px
        color grayColor
        letter-spacing 2px
        @media mediumView
            margin 0
            display flex
            align-items center
            justify-content center
            box-sizing border-box
            height 50px
            color #bbb
            &:not(:last-child)
                border-bottom 1px solid #363535
        @media smallView
            &.js-top-button-1
                display none
        @media largeView
            &:hover
                color blueColor

.top-video
    position relative
    height 100vh
    box-sizing border-box
    padding-top 92px
    @media mediumView
        height 720px
        padding 0
        border-top 92px solid white
    @media smallView
        height 516px
        border-top 56px solid white
    .content-center
        @media smallView
            margin 0
        &:after
            content ''
            position absolute
            width 1px
            height 117px
            background rgba(white, 0.2)
            top 0
            left 20px
            display none
            @media smallView
                display block
    &_img
        position absolute
        width 100%
        height 100%
        top 0
        left 0
        object-fit cover
        display none
        &.medium-view
            @media mediumView
                display block
            @media smallView
                display none
        &.small-view
            @media smallView
                display block
    &_video
        position absolute
        width 100%
        height 100%
        left 0
        top 0
        overflow hidden
        @media mediumView
            display none
        video
            position absolute
            left 0
            top 0
            min-height 100%
            min-width 100%
    &:after
        content ''
        width 100%
        height 1px
        position absolute
        left 0
        top 50%
        margin-top (92px / 2)
        background rgba(white, 0.2)
        @media mediumView
            top 335px
        @media smallView
            top 6px
            width 38%
    &:before
        content ''
        width 38%
        height 1px
        position absolute
        right 0
        top 6px
        margin-top (92px / 2)
        background rgba(white, 0.2)
        z-index 2
        display none
        @media smallView
            display block
    &-content
        height 100%
        position relative
        border-right 1px solid rgba(white, 0.2)
        @media mediumView
            flex-direction column
            box-sizing border-box
            padding-top 40px
            border none
        @media smallView
            padding-top 28px
        &:before
            content ''
            position absolute
            left 0
            bottom 0
            height 50%
            width 645px
            background rgba(yellowColor, 0.55)
            @media mediumView
                left 50%
                transform translate(-50%)
            @media smallView
                left 0
                transform none
                width 100%
        &:after
            content ''
            position absolute
            width 1px
            height 100%
            background rgba(white, 0.2)
            top 0
            right 307px
            @media mediumView
                right 100px
                z-index 2
            @media smallView
                height 117px
                right 20px
        &-info
            flex 0 0 645px
            height 495px
            background yellowColor
            color white
            position relative
            padding 0 45px
            box-sizing border-box
            margin-right 147px
            @media mediumView
                order 2
                flex 0 0 397px
                width 645px
                margin 0
                justify-content flex-start
                padding-top 35px
            @media smallView
                width 100%
                flex 0 0 295px
                align-items center
                padding 25px 20px 0
                box-shadow 0 -10px 0 rgba(yellowColor, 0.55)
            &_desc
                font-size 14px
                text-transform uppercase
                font-family fontBold
                margin-bottom 20px
                letter-spacing 5px
                @media smallView
                    display flex
                    flex-direction column
                    align-items center
                    font-size 12px
                    margin-bottom 25px
                span
                    opacity 0.5
                    font-family fontReg
                    letter-spacing 3px
                    margin-left 10px
                    @media smallView
                        margin-left 0
                        margin-top 7px
            &_title
                font-size 70px
                font-family fontLight
                letter-spacing 3px
                margin-bottom 35px
                @media smallView
                    font-size 36px
                    margin-bottom 15px
                    letter-spacing 2px
            &_text
                font-family fontLight
                line-height 1.6
                width 360px
                @media smallView
                    width auto
                    text-align center
                    font-size 14px
            &_num
                position absolute
                bottom 0
                right 0
                font-size 48px
                font-family fontBold
                opacity 0.2
                line-height 0.7
                letter-spacing 1px
                @media smallView
                    font-size 24px
            &-down-but
                position absolute
                cursor pointer
                user-select none
                width 80px
                height 72px
                bottom -35px
                left 43px
                &.view-large
                    @media smallView
                        display none
                &.view-small
                    display none
                    @media smallView
                        display flex 
                @media smallView
                    left 50%
                    transform translate(-50%)
                    bottom -45px
                &_arrow
                    flex 0 0 1px
                    margin-right 12px
                    background rgba(white, 0.8)
                    position relative
                    animation 1s arrowHeight infinite linear
                    @media smallView
                        margin 0
                        left 50%
                        transform translate(-50%)
                        animation 1s arrowHeightSmall infinite linear
                    &:before,
                    &:after
                        content ''
                        position absolute
                        bottom 0
                        width 1px
                        height 6px
                        background rgba(white, 0.8)
                    &:before
                        transform rotate(-45deg)
                        left (-6px / 2)
                    &:after
                        transform rotate(45deg)
                        left (6px / 2)    
                &_text
                    font-size 12px
                    color rgba(white, 0.8)
                    font-family fontLight
                    padding-top 10px
                    @media smallView
                        display none
            .bottom-triangle-figure
                right 230px
                top 58px
                @media mediumView
                    display none
            .left-triangle-figure
                right 170px
                bottom 80px    
                @media mediumView
                    right initial
                    left 205px
                @media smallView
                    display none
        &_video-but
            cursor pointer
            flex 0 0 125px
            height 125px
            background rgba(black, 0.4)
            border-radius 65px
            box-shadow 0 2px 2px rgba(black, 0.2)
            animation pulse 2s infinite
            z-index 2
            box-sizing border-box
            padding-left 8px
