.layout-header
    height 92px
    background white
    position fixed
    width 100%
    top 0
    left 0
    z-index 100
    
.header-content
    height 100%
    padding 0 30px
    &-buttons
        flex 1 0 0
        &_reg
            flex 0 0 130px

.top-menu
    margin-right 45px
    &_button
        cursor pointer
        user-select none
        font-size 10px
        text-transform uppercase
        margin-left 50px
        color grayColor
        letter-spacing 2px
        &:hover
            color blueColor

.layout-content
    margin-bottom 35px

.top-video
    position relative
    height 100vh
    box-sizing border-box
    padding-top 92px
    &_img
        position absolute
        width 100%
        height 100%
        top 0
        left 0
        object-fit cover
    &:after
        content ''
        width 100%
        height 1px
        position absolute
        left 0
        top 50%
        margin-top (92px / 2)
        background rgba(white, 0.2)
    &-content
        height 100%
        position relative
        border-right 1px solid rgba(white, 0.2)
        &:before
            content ''
            position absolute
            left 0
            bottom 0
            height 50%
            width 645px
            background rgba(yellowColor, 0.55)
        &:after
            content ''
            position absolute
            width 1px
            height 100%
            background rgba(white, 0.2)
            top 0
            right 307px
        &-info
            flex 0 0 645px
            height 495px
            background yellowColor
            color white
            position relative
            padding 0 45px
            box-sizing border-box
            margin-right 147px
            &_desc
                font-size 14px
                text-transform uppercase
                font-family fontBold
                margin-bottom 20px
                letter-spacing 5px
                span
                    opacity 0.5
                    font-family fontReg
                    letter-spacing 3px
                    margin-left 10px
            &_title
                font-size 70px
                font-family fontLight
                letter-spacing 3px
                margin-bottom 35px
            &_text
                font-family fontLight
                line-height 1.6
                width 360px
            &_num
                position absolute
                bottom 0
                right 0
                font-size 48px
                font-family fontBold
                opacity 0.2
                line-height 0.7
                letter-spacing 1px
            &-down-but
                position absolute
                cursor pointer
                user-select none
                width 80px
                height 72px
                bottom -35px
                left 43px
                &_arrow
                    flex 0 0 1px
                    margin-right 12px
                    background rgba(white, 0.8)
                    position relative
                    animation 1s arrowHeight infinite linear
                    &:before,
                    &:after
                        content ''
                        position absolute
                        bottom 0
                        width 1px
                        height 6px
                        background rgba(white, 0.8)
                    &:before
                        transform rotate(-45deg)
                        left (-6px / 2)
                    &:after
                        transform rotate(45deg)
                        left (6px / 2)    
                &_text
                    font-size 12px
                    color rgba(white, 0.8)
                    font-family fontLight
                    padding-top 10px
            .bottom-triangle-figure
                right 230px
                top 58px
            .left-triangle-figure
                right 170px
                bottom 80px    
        &_video-but
            cursor pointer
            flex 0 0 125px
            height 125px
            background rgba(black, 0.4)
            border-radius 65px
            box-shadow 0 2px 2px rgba(black, 0.2)
            animation pulse 2s infinite
            z-index 2
            &:before
                content ''
                width 0
                height 0
                border-style solid
                border-width 21.5px 0 21.5px 27px
                border-color transparent transparent transparent white
        .round-figure
            top 270px
            right 411px
        .square-figure
            right 0
            top 205px

